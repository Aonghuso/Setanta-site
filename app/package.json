{
  "name": "try-setanta",
  "description": "try-setanta.ie AppEngine project",
  "scripts": {
    "setup": "npm install",
    "build": "./buildtut.sh && tsc && webpack",
    "new-build": "make new_dist",
    "clean": "rm -rf jsbuild dist tutout",
    "test": "eslint --ext .ts,.js --max-warnings=0 src/ && cd tut/ && awk -f scripts/test.awk *.md",
    "launch-local": "docker-compose -f docker/docker-compose.yml up -d",
    "launch-new-local-light": "webpack serve -c webpack.new-build.config.js --host=0.0.0.0 --mode=development",
    "launch-local-light": "tsc && webpack serve --host=0.0.0.0 --mode=development",
    "launch-test-env-gcloud": "docker run --rm -it --volumes-from gcloud-config -v \"$PWD:/app\" -w /app gcr.io/google.com/cloudsdktool/cloud-sdk bash",
    "build-tut": "./buildtut.sh",
    "lint": "eslint",
    "lint-all": "eslint --ext .ts,.js src/"
  },
  "dependencies": {
    "@polymer/iron-icon": "^3.0.1",
    "@polymer/iron-icons": "^3.0.1",
    "@polymer/paper-button": "^3.0.1",
    "@polymer/paper-card": "^3.0.1",
    "@polymer/paper-icon-button": "^3.0.2",
    "@polymer/paper-tabs": "^3.1.0",
    "@types/codemirror": "0.0.104",
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "codemirror": "^5.59.3",
    "lit-element": "^2.4.0",
    "setanta": "^0.10.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "copy-webpack-plugin": "^7.0.0",
    "eslint": "^7.20.0",
    "typescript": "^4.1.5",
    "webpack": "^5.23.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  }
}
